<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/p_color_orange_pressed" />
            <corners android:radius="30.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/p_color_orange_pressed" />
            <corners android:radius="30.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1.0dip" android:color="@color/p_color_orange" />
            <corners android:radius="30.0dip" />
        </shape>
    </item>
</selector>